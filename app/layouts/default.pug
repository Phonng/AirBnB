    img.header__poster(src="https://a0.muscache.com/4ea/air/r:w775-h300-sfit,e:fjpg-c80/pictures/a2ff3a00-d803-4255-ab84-4daaef3e88b6.jpg")

    .container.contain
      .header
        .row
          .col-xl-6.col-md-12
            .header__block.rounded
              .header__title
                h1 Find home on Airbnb 
                p Discover private rooms or entire accommodations, perfectly adapted to any type of trip.
                br
              p 

                strong OR
                  form.form-group
                    input.form-control(type="text", placeholder="Destination,ville,adresse")
                    .row
                      .col-md-6.mt-2
                        p
                          strong ARRIVAL
                        input.form-control(type="datetime-local", placeholder="Destination,ville,adresse")
                      .col-md-6.mt-2
                        p
                          strong DEPARTURE
                        input.form-control(type="datetime-local")
                      .col-md-12.mt-2
                        p 
                          strong TRAVELERS
                        select.form-control
                          option 1
                          option 2
                          option 3
                          option 4
                      .col-md-12.mt-4
                        button.btn.btn-success.header__button SEARCH  

      block content
      block review
      block benefit
      block product
      block addDate
    footer.font-small.indigo 
      .container
        .footer 
          .row
            .col-md-3.mx-auto 
